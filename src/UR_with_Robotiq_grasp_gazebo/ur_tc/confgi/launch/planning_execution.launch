<launch>

  <rosparam command="load" file="$(find confgi)/config/joint_names.yaml"/>

  <include file="$(find confgi)/launch/planning_context.launch" >
    <arg name="load_robot_description" value="true" />
  </include>

  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher">
    <param name="/use_gui" value="false"/>
    <rosparam param="/source_list">[/joint_states]</rosparam>
  </node>

  <include file="$(find confgi)/launch/move_group.launch">
    <arg name="publish_monitored_planning_scene" value="true" />
  </include>



  <include file="$(find confgi)/launch/moveit_rviz.launch">
    <arg name="config" value="true"/>
  </include>

</launch>
